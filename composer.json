{
    "name": "lanceedansantos/xendivel",
    "description": "A Laravel package to easily integrate Xendit payment gateway. It supports credit and debit cards, and e-wallet payments and custom invoices, queued notifications, webhook listeners and more.",
    "keywords": ["laravel"],
    "license": "MIT",
    "authors": [
        {
            "role": "Full-Stack Web Developer",
            "name": "Glenn Raya",
            "email": "rayaglenn@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/LanceEdanSantos/xendivel"
        }
    ],
    "require": {
        "php": "~8.2.0|~8.3.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^12.0",
        "nesbot/carbon": "^3.4.0",
        "spatie/browsershot": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.31"
    },
    "autoload": {
        "psr-4": {
            "GlennRaya\\Xendivel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GlennRaya\\Xendivel\\XendivelServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
